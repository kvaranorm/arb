if game.PlaceId == 96063223159256 then
    local a = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
    local b = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
    local c = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

    local d = a:CreateWindow({
        Title = "ARB: Script v2 (Testing)",
        SubTitle = "by xyro",
        TabWidth = 160,
        Size = UDim2.fromOffset(580, 460),
        Acrylic = true,
        Theme = "Dark",
        MinimizeKey = Enum.KeyCode.LeftControl
    })

    local e = {
        A = d:AddTab({Title = "Spinning", Icon = "rotate-ccw"}),
        B = d:AddTab({Title = "Players", Icon = "users"}),
        C = d:AddTab({Title = "Meditation", Icon = "circle"}),
        D = d:AddTab({Title = "NPCs", Icon = "user"}),
        E = d:AddTab({Title = "Codes", Icon = "gift"}),
        F = d:AddTab({Title = "Settings", Icon = "settings"})
    }

    local f = a.Options

    local function g()
        local h = game.Players.LocalPlayer.Character
        return h and h:FindFirstChild("HumanoidRootPart") or nil
    end

    local i = false
    e.D:AddToggle("X1", {
        Title = "Auto Grip NPCs",
        Default = false,
        Description = "Grips nearest NPC repeatedly"
    }):OnChanged(function(j)
        i = j
        task.spawn(function()
            while i do
                local k = game.Players.LocalPlayer
                local l = g()
                local m = workspace:FindFirstChild("Lives")

                if l and m then
                    local n, o = nil, math.huge
                    for _, p in ipairs(m:GetChildren()) do
                        local q = p:FindFirstChild("HumanoidRootPart")
                        if p.Name == "RegularNationNpc" and q then
                            local r = (q.Position - l.Position).Magnitude
                            if r < o then
                                n, o = p, r
                            end
                        end
                    end

                    if n then
                        local s = {"Kill", n, l.Position + Vector3.new(1, 2, 0)}
                        for _ = 1, 3 do
                            k.Character.Scripts.Server.Grip.Event:FireServer(unpack(s))
                            task.wait(0.1)
                        end
                    end
                end
                task.wait(0.5)
            end
        end)
    end)

    local t = false
    local u = {KINcAgU = true, topaisrare = true, xylrr = true}

    e.B:AddToggle("X2", {
        Title = "Auto Grip Players",
        Default = false,
        Description = "Spams grip on nearest player not in whitelist within 20 studs"
    }):OnChanged(function(v)
        t = v
        task.spawn(function()
            while t do
                local w = game.Players.LocalPlayer
                local x = g()
                if not x then task.wait(0.5) else
                    local y, z = nil, 20
                    for _, A in ipairs(game.Players:GetPlayers()) do
                        if A ~= w and not u[A.Name] and A.Character and A.Character:FindFirstChild("HumanoidRootPart") then
                            local B = A.Character.HumanoidRootPart
                            local C = (B.Position - x.Position).Magnitude
                            if C <= z then
                                y, z = A.Character, C
                            end
                        end
                    end

                    if y then
                        local D = {"Kill", y, x.Position + Vector3.new(1, 2, 0)}
                        w.Character.Scripts.Server.Grip.Event:FireServer(unpack(D))
                    end
                    task.wait(0.1)
                end
            end
        end)
    end)

    e.C:AddToggle("X3", {
        Title = "Auto Meditate",
        Default = false,
        Description = "Automatically meditates for you"
    }):OnChanged(function(E)
        task.spawn(function()
            while E do
                game.ReplicatedStorage.RemoteEvents.Meditate:FireServer("Start")
                for _ = 1, 5 do
                    game.ReplicatedStorage.RemoteEvents["Create Key"]:FireServer("Complete")
                    task.wait(0.1)
                end
                task.wait(1.5)
            end
        end)
    end)

    local F = {"wherestheavatarSpins", "25SpinsYay"}
    e.E:AddButton({
        Title = "Redeem Spin Codes",
        Description = "Redeem all spin codes",
        Callback = function()
            for _, G in ipairs(F) do
                game:GetService("ReplicatedStorage").RemoteEvents.RedeemCode:InvokeServer(G)
                task.wait(0.5)
            end
        end
    })

    local H = {"Yakoda", "Sozin", "Gyatso", "Lozai", "Vaan", "Tarrlok"}
    e.A:AddDropdown("X4", {
        Title = "Target Families",
        Values = H,
        Multi = true,
        Default = {}
    })

    e.A:AddToggle("X5", {
        Title = "Auto Spin (Family)",
        Default = false,
        Description = "Spins until a selected family is achieved"
    }):OnChanged(function(I)
        task.spawn(function()
            local J = game.Players.LocalPlayer
            while I do
                local K = J:FindFirstChild("Family")
                local L = K and K.Value or nil
                local M = f.X4.Value

                if L and table.find(M, L) then
                    f.X5:SetValue(false)
                    break
                end

                game.ReplicatedStorage.RemoteEvents.Spin:FireServer("Family")
                task.wait(7.5)
            end
        end)
    end)

    local N = {"Fire", "Water", "Earth", "Air"}
    local O = {
        "flameohotman",
        "thatsroughbuddy",
        "rewrittingmaybesoon",
        "ElementRerollYay"
    }

    e.E:AddDropdown("X6", {
        Title = "Target Elements",
        Values = N,
        Multi = true,
        Default = {}
    })

    e.E:AddToggle("X7", {
        Title = "Auto Redeem Element Codes",
        Default = false,
        Description = "Redeem element codes until you get one of the selected elements"
    }):OnChanged(function(P)
        task.spawn(function()
            local Q = game.Players.LocalPlayer
            while P do
                local R = Q:FindFirstChild("Element")
                local S = R and R.Value or nil
                local T = f.X6.Value

                if S and table.find(T, S) then
                    f.X7:SetValue(false)
                    break
                end

                for _, U in ipairs(O) do
                    game:GetService("ReplicatedStorage").RemoteEvents.RedeemCode:InvokeServer(U)
                    task.wait(1.5)
                end
            end
        end)
    end)

    b:SetLibrary(a)
    c:SetLibrary(a)
    b:IgnoreThemeSettings()
    b:SetIgnoreIndexes({})
    c:SetFolder("FluentScriptHub")
    b:SetFolder("FluentScriptHub/specific-game")
    c:BuildInterfaceSection(e.F)
    b:BuildConfigSection(e.F)

    d:SelectTab(1)
    a:Notify({
        Title = "Fluent",
        Content = "Script Loaded Successfully!",
        Duration = 8
    }
