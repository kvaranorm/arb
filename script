if game.PlaceId==96063223159256 then
local a=loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local b=loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local c=loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()
local d="ARB: Script v2 (Testing)"
local e=a:CreateWindow({Title=d,SubTitle="by xyro",TabWidth=160,Size=UDim2.fromOffset(580,460),Acrylic=true,Theme="Dark",MinimizeKey=Enum.KeyCode.LeftControl})
local f={
Spinning=e:AddTab({Title="Spinning",Icon="rotate-ccw"}),
Players=e:AddTab({Title="Players",Icon="users"}),
Meditation=e:AddTab({Title="Meditation",Icon="circle"}),
NPCs=e:AddTab({Title="NPCs",Icon="user"}),
Codes=e:AddTab({Title="Codes",Icon="gift"}),
Settings=e:AddTab({Title="Settings",Icon="settings"})
}
local g=a.Options
local function h()
local i=game.Players.LocalPlayer.Character
return i and i:FindFirstChild("HumanoidRootPart")
end
local j=false
f.NPCs:AddToggle("AutoGripToggle",{Title="Auto Grip NPCs",Default=false,Description="Grips nearest npc repeatedly"}):OnChanged(function(k)
j=k
task.spawn(function()
while j do
local l=game.Players.LocalPlayer.Character
local m=h()
local n=workspace:FindFirstChild("Lives")
if m and n then
local o,p=nil,math.huge
for q,r in ipairs(n:GetChildren())do
local s=r:FindFirstChild("HumanoidRootPart")
if r.Name=="RegularNationNpc"and s then
local t=(s.Position-m.Position).Magnitude
if t<p then
o,p=r,t
end
end
end
if o then
local u={"Kill",o,m.Position+Vector3.new(1,2,0)}
for v=1,3 do
l.Scripts.Server.Grip.Event:FireServer(unpack(u))
task.wait(0.1)
end
end
end
task.wait(0.5)
end
end)
end)
local w=false
local x={["KINcAgU"]=true,["topaisrare"]=true,["xylrr"]=true}
f.Players:AddToggle("AutoGripPlayersToggle",{Title="Auto Grip Players",Default=false,Description="Spams grip on nearest player not in whitelist within 20 studs"}):OnChanged(function(y)
w=y
task.spawn(function()
while w do
local z=game.Players.LocalPlayer
local A=h()
if not A then task.wait(0.5)continue end
local B,C=nil,20
for D,E in ipairs(game.Players:GetPlayers())do
if E~=z and not x[E.Name] and E.Character and E.Character:FindFirstChild("HumanoidRootPart")then
local F=(E.Character.HumanoidRootPart.Position-A.Position).Magnitude
if F<=C then
B,C=E.Character,F
end
end
end
if B then
local G={"Kill",B,A.Position+Vector3.new(1,2,0)}
z.Character.Scripts.Server.Grip.Event:FireServer(unpack(G))
end
task.wait(0.1)
end
end)
end)
f.Meditation:AddToggle("AutoMeditateToggle",{Title="Auto Meditate",Default=false,Description="Automatically meditates for you !!DISCLAIMER!! YOU HAVE TO SIT ON A MAT AND CLICK MEDITATE THEN TURN IT ON"}):OnChanged(function(H)
task.spawn(function()
while H do
game.ReplicatedStorage.RemoteEvents.Meditate:FireServer("Start")
for I=1,5 do
game.ReplicatedStorage.RemoteEvents["Create Key"]:FireServer("Complete")
task.wait(0.1)
end
wait(1.5)
end
end)
end)
local J={"wherestheavatarSpins","25SpinsYay"}
local K={"flameohotman","thatsroughbuddy","rewrittingmaybesoon","ElementRerollYay"}
f.Codes:AddButton({Title="Redeem Spin Codes",Description="Redeem all spin codes",Callback=function()
for L,M in ipairs(J)do
game:GetService("ReplicatedStorage").RemoteEvents.RedeemCode:InvokeServer(M)
task.wait(0.5)
end
end})
local N=false
local O={"Yakoda","Sozin","Gyatso","Lozai","Vaan","Tarrlok"}
f.Spinning:AddDropdown("SpinFamily",{Title="Target Families",Values=O,Multi=true,Default={}})
f.Spinning:AddToggle("AutoSpin",{Title="Auto Spin (Family)",Default=false,Description="Spins until a selected family is achieved"}):OnChanged(function(P)
N=P
task.spawn(function()
local Q=game.Players.LocalPlayer
while N do
local R=Q:FindFirstChild("Family")
local S=R and R.Value or nil
local T=g.SpinFamily.Value
if S and table.find(T,S)then
g.AutoSpin:SetValue(false)
N=false
break
end
game.ReplicatedStorage.RemoteEvents.Spin:FireServer("Family")
task.wait(7.5)
end
end)
end)
b:SetLibrary(a)
c:SetLibrary(a)
b:IgnoreThemeSettings()
b:SetIgnoreIndexes({})
c:SetFolder("FluentScriptHub")
b:SetFolder("FluentScriptHub/specific-game")
c:BuildInterfaceSection(f.Settings)
b:BuildConfigSection(f.Settings)
e:SelectTab(1)
a:Notify({Title="Fluent",Content="Script Loaded Successfully!",Duration=8})
b:LoadAutoloadConfig()
end
