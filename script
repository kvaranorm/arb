if game.PlaceId == 96063223159256 then
    -- Load libraries
    local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
    local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
    local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

    local windowTitle = "ARB: Script v2 (Testing)"
    local window = Fluent:CreateWindow({
        Title = windowTitle,
        SubTitle = "by xyro",
        TabWidth = 160,
        Size = UDim2.fromOffset(580, 460),
        Acrylic = true,
        Theme = "Dark",
        MinimizeKey = Enum.KeyCode.LeftControl
    })

    local tabs = {
        Spinning = window:AddTab({Title = "Spinning", Icon = "rotate-ccw"}),
        Players = window:AddTab({Title = "Players", Icon = "users"}),
        Meditation = window:AddTab({Title = "Meditation", Icon = "circle"}),
        NPCs = window:AddTab({Title = "NPCs", Icon = "user"}),
        Codes = window:AddTab({Title = "Codes", Icon = "gift"}),
        Settings = window:AddTab({Title = "Settings", Icon = "settings"})
    }

    local options = Fluent.Options

    local function getHumanoidRootPart()
        local character = game.Players.LocalPlayer.Character
        if character then
            return character:FindFirstChild("HumanoidRootPart")
        end
        return nil
    end

    -- Auto Grip NPCs toggle
    local autoGripNPCsEnabled = false
    tabs.NPCs:AddToggle("AutoGripToggle", {
        Title = "Auto Grip NPCs",
        Default = false,
        Description = "Grips nearest NPC repeatedly"
    }):OnChanged(function(enabled)
        autoGripNPCsEnabled = enabled
        task.spawn(function()
            while autoGripNPCsEnabled do
                local player = game.Players.LocalPlayer
                local hrp = getHumanoidRootPart()
                local livesFolder = workspace:FindFirstChild("Lives")

                if hrp and livesFolder then
                    local closestNPC, shortestDistance = nil, math.huge
                    for _, npc in ipairs(livesFolder:GetChildren()) do
                        local npcHRP = npc:FindFirstChild("HumanoidRootPart")
                        if npc.Name == "RegularNationNpc" and npcHRP then
                            local dist = (npcHRP.Position - hrp.Position).Magnitude
                            if dist < shortestDistance then
                                closestNPC = npc
                                shortestDistance = dist
                            end
                        end
                    end

                    if closestNPC then
                        local args = {"Kill", closestNPC, hrp.Position + Vector3.new(1, 2, 0)}
                        for _ = 1, 3 do
                            player.Character.Scripts.Server.Grip.Event:FireServer(unpack(args))
                            wait()
                        end
                    end
                end
                wait()
            end
        end)
    end)

    -- Auto Grip Players toggle
    local autoGripPlayersEnabled = false
    local whitelist = {["KINcAgU"] = true, ["topaisrare"] = true, ["xylrr"] = true}

    tabs.Players:AddToggle("AutoGripPlayersToggle", {
        Title = "Auto Grip Players",
        Default = false,
        Description = "Spams grip on nearest player not in whitelist within 20 studs"
    }):OnChanged(function(enabled)
        autoGripPlayersEnabled = enabled
        task.spawn(function()
            while autoGripPlayersEnabled do
                local localPlayer = game.Players.LocalPlayer
                local hrp = getHumanoidRootPart()
                if not hrp then
                    task.wait(0.5)
                else
                    local targetPlayerCharacter, closestDistance = nil, 20
                    for _, player in ipairs(game.Players:GetPlayers()) do
                        if player ~= localPlayer and not whitelist[player.Name] and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                            local playerHRP = player.Character.HumanoidRootPart
                            local dist = (playerHRP.Position - hrp.Position).Magnitude
                            if dist <= closestDistance then
                                targetPlayerCharacter = player.Character
                                closestDistance = dist
                            end
                        end
                    end

                    if targetPlayerCharacter then
                        local args = {"Kill", targetPlayerCharacter, hrp.Position + Vector3.new(1, 2, 0)}
                        localPlayer.Character.Scripts.Server.Grip.Event:FireServer(unpack(args))
                    end
                    task.wait(0.1)
                end
            end
        end)
    end)

    -- Auto Meditate toggle
    tabs.Meditation:AddToggle("AutoMeditateToggle", {
        Title = "Auto Meditate",
        Default = false,
        Description = "Automatically meditates for you !!DISCLAIMER!! YOU HAVE TO SIT ON A MAT AND CLICK MEDITATE THEN TURN IT ON"
    }):OnChanged(function(enabled)
        task.spawn(function()
            while enabled do
                game.ReplicatedStorage.RemoteEvents.Meditate:FireServer("Start")
                for _ = 1, 5 do
                    game.ReplicatedStorage.RemoteEvents["Create Key"]:FireServer("Complete")
                    task.wait(0.1)
                end
                task.wait(1.5)
            end
        end)
    end)

    -- Setup SaveManager and InterfaceManager
    SaveManager:SetLibrary(Fluent)
    InterfaceManager:SetLibrary(Fluent)
    SaveManager:IgnoreThemeSettings()
    SaveManager:SetIgnoreIndexes({})
    InterfaceManager:SetFolder("FluentScriptHub")
    SaveManager:SetFolder("FluentScriptHub/specific-game")
    InterfaceManager:BuildInterfaceSection(tabs.Settings)
    SaveManager:BuildConfigSection(tabs.Settings)

    window:SelectTab(1)

    Fluent:Notify({
        Title = "Fluent",
        Content = "Script Loaded Successfully!",
        Duration = 8
    })

    SaveManager:LoadAutoloadConfig()

    -- ====== START OF NEW CODE ======
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local LocalPlayer = game.Players.LocalPlayer

    -- Families, Traits and Elements lists
    local Families = {"Yakoda", "Lozai", "Leifong", "Tarrlok", "Sozin", "Hama", "Gyatso", "Vaan"}
    local Traits = {"Agile", "Deadly", "Evil", "Athlete", "Limb", "Prodigy", "Talented"}
    local Elements = {"Water", "Earth", "Air", "Fire"}

    local SpinCodes = {"wherestheavatarSpins", "25SpinsYay"}
    local ElementCodes = {"flameohotman", "thatsroughbuddy", "rewrittingmaybesoon", "wherestheavatarSpins", "ElementRerollYay", "25SpinsYay"}

    -- Auto Spin variables
    local autoSpinFamilyEnabled = false
    local autoSpinTraitEnabled = false
    local selectedFamilies = {}
    local selectedTraits = {}

    -- Auto Redeem variables
    local autoRedeemSpinEnabled = false
    local autoRedeemElementEnabled = false
    local targetElement = Elements[1]

    -- Function to spin for Family
    local function spinForFamily()
        local spinRemote = ReplicatedStorage:WaitForChild("RemoteEvents"):WaitForChild("Spin")
        while autoSpinFamilyEnabled and not Fluent.Unloaded do
            spinRemote:FireServer("Family")
            task.wait(7.5)
            local familyValue = LocalPlayer:FindFirstChild("Family") and LocalPlayer.Family.Value or ""
            for _, fam in ipairs(selectedFamilies) do
                if familyValue:find(fam) then
                    autoSpinFamilyEnabled = false
                    Fluent:Notify({Title = "Family Match", Content = "You rolled family: " .. fam, Duration = 5})
                    return
                end
            end
            task.wait(0.5)
        end
    end

    -- Function to spin for Trait
    local function spinForTrait()
        local spinRemote = ReplicatedStorage:WaitForChild("RemoteEvents"):WaitForChild("Spin")
        while autoSpinTraitEnabled and not Fluent.Unloaded do
            spinRemote:FireServer("Trait")
            task.wait(7.5)
            local traitStat = LocalPlayer:FindFirstChild("Player Stats") and LocalPlayer["Player Stats"]:FindFirstChild("Trait")
            local traitValue = traitStat and traitStat.Value or ""
            for _, trait in ipairs(selectedTraits) do
                if traitValue:find(trait) then
                    autoSpinTraitEnabled = false
                    Fluent:Notify({Title = "Trait Match", Content = "You rolled trait: " .. trait, Duration = 5})
                    return
                end
            end
            task.wait(0.5)
        end
    end

    -- Auto Spin UI Setup on Spinning Tab
    tabs.Spinning:AddParagraph({Title = "Auto Spin Settings", Content = "Select families and traits to spin for, then toggle each separately."})

    local familyDropdown = tabs.Spinning:AddDropdown("FamilyDropdown", {
        Title = "Select Families",
        Description = "Choose families to spin for (multiple allowed)",
        Values = Families,
        Multi = true,
        Default = {}
    })
    familyDropdown:OnChanged(function(value)
        selectedFamilies = {}
        for fam, selected in pairs(value) do
            if selected then table.insert(selectedFamilies, fam) end
        end
    end)

    local traitDropdown = tabs.Spinning:AddDropdown("TraitDropdown", {
        Title = "Select Traits",
        Description = "Choose traits to spin for (multiple allowed)",
        Values = Traits,
        Multi = true,
        Default = {}
    })
    traitDropdown:OnChanged(function(value)
        selectedTraits = {}
        for trait, selected in pairs(value) do
            if selected then table.insert(selectedTraits, trait) end
        end
    end)

    tabs.Spinning:AddToggle("AutoSpinFamilyToggle", {
        Title = "Auto Spin Family",
        Description = "Spins only for selected families",
        Default = false
    }):OnChanged(function(state)
        autoSpinFamilyEnabled = state
        if state then task.spawn(spinForFamily) end
    end)

    tabs.Spinning:AddToggle("AutoSpinTraitToggle", {
        Title = "Auto Spin Trait",
        Description = "Spins only for selected traits",
        Default = false
    }):OnChanged(function(state)
        autoSpinTraitEnabled = state
        if state then task.spawn(spinForTrait) end
    end)

    -- Auto Redeem Spin Codes Toggle in Codes Tab
    tabs.Codes:AddToggle("AutoRedeemSpin", {
        Title = "Auto Redeem Spin Codes",
        Description = "Redeem all spin codes automatically once",
        Default = false
    }):OnChanged(function(state)
        autoRedeemSpinEnabled = state
        if state then
            task.spawn(function()
                for _, code in ipairs(SpinCodes) do
                    ReplicatedStorage.RemoteEvents.RedeemCode:InvokeServer(code)
                    task.wait(0.5)
                end
                autoRedeemSpinEnabled = false
            end)
        end
    end)

    -- Element dropdown
    local elementDropdown = tabs.Codes:AddDropdown("ElementDropdown", {
        Title = "Select Target Element",
        Description = "Select element to stop auto redeem",
        Values = Elements,
        Multi = false,
        Default = 1
    })
    elementDropdown:OnChanged(function(value)
        targetElement = value
    end)
    elementDropdown:SetValue(targetElement)

    -- Auto Redeem Element Codes Toggle
    tabs.Codes:AddToggle("AutoRedeemElement", {
        Title = "Auto Redeem Element Codes",
        Description = "Redeem element codes until target element is rolled",
        Default = false
    }):OnChanged(function(state)
        autoRedeemElementEnabled = state
        if state then
            task.spawn(function()
                while autoRedeemElementEnabled do
                    ReplicatedStorage.RemoteEvents.RedeemCode:InvokeServer(ElementCodes[math.random(1, #ElementCodes)])
                    task.wait(1)
                    local bendingVal = LocalPlayer:FindFirstChild("Bending") and LocalPlayer.Bending.Value or ""
                    if bendingVal == targetElement then
                        autoRedeemElementEnabled = false
                        Fluent:Notify({Title = "Element Match", Content = "You got element: " .. targetElement, Duration = 5})
                        break
                    end
                end
            end)
        end
    end)
end
